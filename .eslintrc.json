{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    // Custom rules or overrides
    // Custom rules or overrides
    "@typescript-eslint/no-unused-vars": [
        1,
        {
            "argsIgnorePattern": "_*"
        }
    ]
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": [
    "@typescript-eslint"
  ]
}
